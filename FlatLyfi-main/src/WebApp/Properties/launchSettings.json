{
    "$schema": "http://json.schemastore.org/launchsettings.json",
    "profiles": {
        "http": {
            "commandName": "Project",
            "dotnetRunMessages": true,
            "launchBrowser": true,
            "applicationUrl": "http://localhost:5045",
            "hotReloadEnabled": true,
            "environmentVariables": {
                "ASPNETCORE_ENVIRONMENT": "Development"
            }
        },
        "https": {
            "commandName": "Project",
            "dotnetRunMessages": true,
            "launchBrowser": true,
            "applicationUrl": "https://localhost:7298;http://localhost:5045",
            "hotReloadEnabled": true,
            "environmentVariables": {
                "ASPNETCORE_ENVIRONMENT": "Development"
            }
        },
        "httpsdev": {
            "commandName": "Project",
            "dotnetRunMessages": true,
            "launchBrowser": true,
            "applicationUrl": "https://localhost:7290;http://localhost:5040",
            "hotReloadEnabled": true,
            "environmentVariables": {
                "ASPNETCORE_ENVIRONMENT": "Development",
                "IdentityUrl": "https://localhost:5243;http://localhost:5223"
            }
        },
        "HotReloadBlazorLauncher": {
            "commandName": "Executable",
            "workingDirectory": "$(ProjectDir)",
            "executablePath": "dotnet",
            "commandLineArgs": "watch run debug --launch-profile httpsdev"
        }
    }
}
